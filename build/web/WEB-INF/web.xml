<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>LoginFilter</filter-name>
        <filter-class>Fliter.LoginFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>LoginFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>Listener.TestListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>controller.Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WorkSpaces</servlet-name>
        <servlet-class>controller.WorkSpaces</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>controller.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyPage</servlet-name>
        <servlet-class>controller.MyPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Manage</servlet-name>
        <servlet-class>controller.Manage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>controller.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contact</servlet-name>
        <servlet-class>controller.Contact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyDataUpdate</servlet-name>
        <servlet-class>controller.MyDataUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyDataDelete</servlet-name>
        <servlet-class>controller.MyDataDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyPictureManage</servlet-name>
        <servlet-class>controller.MyPictureManage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyPictureUpload</servlet-name>
        <servlet-class>controller.MyPictureUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PictureDetail</servlet-name>
        <servlet-class>controller.PictureDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WorkSpaces</servlet-name>
        <url-pattern>/WorkSpaces</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyPage</servlet-name>
        <url-pattern>/MyPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Manage</servlet-name>
        <url-pattern>/Manage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contact</servlet-name>
        <url-pattern>/Contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyDataUpdate</servlet-name>
        <url-pattern>/MyDataUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyDataDelete</servlet-name>
        <url-pattern>/MyDataDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyPictureManage</servlet-name>
        <url-pattern>/MyPictureManage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyPictureUpload</servlet-name>
        <url-pattern>/MyPictureUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PictureDetail</servlet-name>
        <url-pattern>/PictureDetail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
